{
    "name": "jean-beru/pipeline-bundle",
    "description": "Pipeline bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Jean-Beru",
            "email": "hubert.lenoir@sensiolabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeanBeru\\PipelineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeanBeru\\PipelineBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "league/pipeline": "^1.0",
        "symfony/config": "^5.0|^6.0",
        "symfony/dependency-injection": "^5.0|^6.0",
        "symfony/http-kernel": "^5.0|^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "suggest": {
        "symfony/event-dispatcher": "Allows event subscription on the stage execution"
    }
}
